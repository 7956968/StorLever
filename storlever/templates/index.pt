<!DOCTYPE html>
<html lang="zh-CN" ng-app="app">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Storlevel</title>
    <link rel="shortcut icon" href="${request.static_url('storlever:static/favicon.ico')}" />
    <link rel="stylesheet" href="${request.static_url('storlever:static/styles/bootstrap.min.css')}" media="screen" charset="utf-8" />
    <link rel="stylesheet" href="${request.static_url('storlever:static/styles/index.css')}">
</head>
<body>
    <!-- Fixed navbar -->
    <header  class="navbar navbar-inverse navbar-fixed-top" role="navigation">
        <div class="container first-container">
            <div class="navbar-header" >
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="#">Storlever</a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav" id="menuList" ng-controller="MenuList">
                    <li ng-repeat="root in roots" class="menu-list-{{root.node_type}}" id="{{root.node_id}}">
                        <a href="#">{{root.text}}</a>
                    </li>
                </ul>
                <ul class="nav navbar-nav navbar-right">
                    <li><a>注销</a></li>
                    <li><a>帮助</a></li>
                </ul>
            </div><!--/.nav-collapse -->
        </div>
    </header>
    <div class="container">
        <div class="row">
            <div class="col-md-3">
                <ul class="list-group" ng-controller="MenuListInter" id="menuListIntermediate">
                    <li ng-repeat="intermediate in intermediates" class="list-group-item menu-list-{{intermediate.node_type}}" id="{{intermediate.node_id}}">
                        <a href="#"><h4>{{intermediate.text}}</h4></a>
                        <ul class="nav nav-list">
                            <li ng-repeat="leaf in intermediate.sub_nodes track by $index" class="menu-list-{{leaf.node_type}}" id="{{leaf.node_id}}">
                                <a href="#">{{leaf.text}}</a>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
            <div class="col-md-9" id="contentContainer">
                <div class="tabbable" id="tabs-64902">
                    <ul class="nav nav-tabs">
                        <li class="active">
                            <a href="#panel-931479" data-toggle="tab">第一部分</a>
                        </li>
                        <li>
                            <a href="#panel-820165" data-toggle="tab">第二部分</a>
                        </li>
                    </ul>
                    <div class="tab-content">
                        <div class="tab-pane active" id="panel-931479">
                            <p>第一部分内容.</p>
                        </div>
                        <div class="tab-pane" id="panel-820165">
                            <p>第二部分内容.</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="${request.static_url('storlever:static/scripts/jquery-2.1.1.min.js')}"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="${request.static_url('storlever:static/scripts/bootstrap.min.js')}"></script>
    <script src="${request.static_url('storlever:static/scripts/angular.min.js')}"></script>
    <script src="${request.static_url('storlever:static/scripts/index.js')}"></script>
  </body>
</html>