      </div>
    </tab>
    <tab heading="网络统计" select="maintain.show()">
      <div class="row row-in-tab">
        <div class="col-sm-12">
          <div class="panel panel-default">
            <div class="panel-body">
            </div>
          </div>
        </div>
      </div>
    </tab>
    <tab heading="系统进程" select="maintain.show()">
      <div class="row row-in-tab">
        <div class="col-sm-12">
          <div class="panel panel-default">
            <div class="panel-body">
            </div>
          </div>
        </div>
      </div>
    </tab>
  </tabset>
</div>
